    public override void CreateNewOutputRows()
    {
        try
        {
            string yamlFilePath = @"C:\Users\remyy\Downloads\BIProject\employees_directory.yaml";
            string yamlContent = File.ReadAllText(yamlFilePath);

            // Parse YAML manually (simple approach)
            ParseYamlAndCreateRows(yamlContent);
        }
        catch (Exception ex)
        {
            // Log error
            bool fireAgain = false;
            ComponentMetaData.FireError(0, "YAML Parser", ex.Message, "", 0, out fireAgain);
        }
    }

    private void ParseYamlAndCreateRows(string yamlContent)
    {
        // Extract employee sections using regex
        string pattern = @"- employee_id: (EMP\d+)\s+job_info:\s+department: ([^\r\n]+)\s+hire_date: '([^']+)'\s+manager_id: ([^\r\n]+)\s+position: ([^\r\n]+)\s+salary: ([^\r\n]+)\s+performance:\s+goals_met: (\d+)\s+last_review_date: '([^']+)'\s+rating: ([^\r\n]+)\s+personal_info:\s+email: ([^\r\n]+)\s+first_name: ([^\r\n]+)\s+last_name: ([^\r\n]+)\s+phone: ([^\r\n]+)";

        MatchCollection matches = Regex.Matches(yamlContent, pattern, RegexOptions.Multiline | RegexOptions.IgnoreCase);

        foreach (Match match in matches)
        {
            Sortie0Buffer.AddRow();
            Sortie0Buffer.EmployeeID = match.Groups[1].Value;
            Sortie0Buffer.Department = match.Groups[2].Value;
            // Handle null manager_id

            Sortie0Buffer.Position = match.Groups[5].Value;
            Sortie0Buffer.Salary = Convert.ToDecimal(match.Groups[6].Value);
            Sortie0Buffer.Email = match.Groups[10].Value;
            Sortie0Buffer.FirstName = match.Groups[11].Value;
            Sortie0Buffer.LastName = match.Groups[12].Value;
        }
    }

